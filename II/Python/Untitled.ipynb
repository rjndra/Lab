   "source": [
    "a = int(input(\"Enter a: \"))\n",
    "b = int(input(\"Enter b:\"))\n",
    "op = input(\"Enter and operator:\")\n",
    "\n",
    "match op:\n",
    "    case \"+\":\n",
    "        print(a+b)\n",
    "    case \"-\":\n",
    "        print(a-b)\n",
    "    case \"*\":\n",
    "        print(a*b)\n",
    "    case \"/\":\n",
    "        print(a/b)\n",
    "    case _:\n",
    "        print(\"Wrong Operator\", op)"
   ]
